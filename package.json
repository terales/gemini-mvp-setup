{
  "name": "test-gemini-setup",
  "version": "0.0.0",
  "description": "For initial experience of gemini setup",
  "scripts": {
    "postinstall": "selenium-standalone install",    
    "test-e2e:ci": "node ./gemini-runner",
    "test-e2e:start-selenium": "selenium-standalone start",
    "test-e2e:run-gemini": "gemini test ./test-e2e-screenshots --reporter flat --reporter html",
    "test-e2e:update-reference": "gemini update ./test-e2e-screenshots"
  },
  "devDependencies": {
    "gemini": "4.18.1",
    "selenium-standalone": "6.2.0"
  },
  "author": "Aleksander Terekhov <terehov.alexander.serg+npm@gmail.com> (https://www.terales.info/)",
  "license": "MIT",
  "repository": {
    "type" : "git",
    "url" : "https://github.com/npm/npm.git"
  }
}
